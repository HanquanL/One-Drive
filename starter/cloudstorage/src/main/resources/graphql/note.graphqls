type Note{
    id: ID!
    title: String!
    content: String!
    createdTime: String!
}

type Query{
    notes: [Note!]!
    noteById(id: ID!): Note!
}

type Mutation{
    createNote(
        title: String!
        content: String!
    ): Note!
    updateNote(
        id: ID!
        title: String!
        content: String!
    ): Note!
    deleteNote(
        id: ID!
    ): Note!
}
